}
abstract class communication implements sharing
{
	abstract void spreadKnowledge();
}
interface sharing
{
	void sharingInformation();
}

class verbalCommunication extends communication
{

	@Override
	public void sharingInformation() {
		// TODO Auto-generated method stub
		
	}

	@Override
	void spreadKnowledge() {
		// TODO Auto-generated method stub
		
	}
	
}
class intraPersonalCommunication extends verbalCommunication 
{
	void reading()
	{
		
	}
	void selfTalking()
	{
		
	}
	void meditating()
	{
		
	}
}
class interPersonalCommunication extends verbalCommunication
{
	void Call()
	{
		
	}

}
class smallGroupCommunication extends verbalCommunication implements sharing
{	
	void meeting()
 {
	
 }

	@Override
	public void sharingInformation() {
		// TODO Auto-generated method stub
		
	}
	
	
}

